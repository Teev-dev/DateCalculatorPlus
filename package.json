{
  "name": "date-calculator-plus",
  "version": "1.0.0",
  "description": "Chrome extension for calculating working days across different countries",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --config config/webpack.config.js --mode development --open",
    "build": "webpack --config config/webpack.config.js --mode production",
    "dev": "webpack serve --config config/webpack.config.js --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "cypress run",
    "cypress:open": "cypress open",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,css,html}\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "date-fns": "^2.x.x"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.x.x",
    "babel-loader": "^8.x.x",
    "css-loader": "^6.x.x",
    "style-loader": "^3.x.x",
    "webpack": "^5.x.x",
    "webpack-cli": "^4.x.x",
    "webpack-dev-server": "^4.x.x",
    "html-webpack-plugin": "^5.x.x",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "eslint": "^8.x.x",
    "prettier": "^2.x.x",
    "babel-jest": "^29.7.0",
    "cypress": "^13.6.2"
  },
  "author": "",
  "license": "ISC"
}
